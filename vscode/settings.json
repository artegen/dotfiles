{
  /* Experimental settings */
  // "[css]": {
  //   "editor.formatOnSave": false
  // },
  "workbench.editor.labelFormat": "short",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "emmet.excludeLanguages": [],
  "emmet.syntaxProfiles": {},
  "emmet.triggerExpansionOnTab": true,
  "html-css-class-completion.enableEmmetSupport": false,
  "csv-preview.separator": ";",
  "git.promptToSaveFilesBeforeCommit": true,
  "jumpy.wordRegexp": "([A-Z]+([0-9a-z])*)|[a-z0-9]{2,}",

  "eslint._legacyModuleResolve": false,
  "eslint.enable": true,
  "eslint.nodePath": null,
  "eslint.run": "onSave",
  "eslint.workingDirectories": [],
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "prettier.disableLanguages": [
    "css" //better use beautify.js
  ],
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.semi": true,
  // "prettier.stylelintIntegration": true, stylelint can't fix issues
  // "prettier.ignorePath": "~/.prettierignore",
  // "prettier.cssEnable": [""],

  // "vetur.format.defaultFormatterOptions": {
  //   "prettier": {
  //     // add your prettier settings here
  //     "singleQuote": true,
  //     "semi": false
  //   }
  // },
  // "vetur.format.defaultFormatter.html": "js-beautify-html",
  // "beautify.language": {
  //   "css": ["css", "scss"]
  // },
  // "spellright.notificationClass": "information",
  //   "[jsonc]": {
  //     "editor.formatOnSave": true
  // },
  "terminal.integrated.confirmOnExit": true,
  "terminal.integrated.scrollback": 30000,
  "flow.runOnEdit": true,
  // "flow.pathToFlow": "./node_modules/.bin",
  "flow.useNPMPackagedFlow": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "500",
  "workbench.fontAliasing": "antialiased",
  "json.schemas": [
    {
      "fileMatch": ["/package.json"],
      "url": "http://json.schemastore.org/package"
    },
    {
      "fileMatch": ["/swagger.json"],
      "url": "http://json.schemastore.org/swagger-2.0"
    }
  ],

  /* Main settings */
  "files.associations": {
    ".htaccess": "shellscript",
    "**/bin/*": "shellscript",
    "*bash*": "shellscript",
    // "*rc": "shellscript",
    "*ssh*": "shellscript",
    "*aliases": "shellscript",
    ".exports": "shellscript",
    ".functions": "shellscript",
    "brewfile": "shellscript",
    "git*": "shellscript",
    "Jenkinsfile*": "groovy",
    "muc": "shellscript",
    ".env.*": "shellscript",
    "*.json5": "json",
    ".eslintrc": "json",
    "**/fastlane/*": "ruby"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "files.watcherExclude": {
    "**/.git/objects": true,
    "**/dist": true,
    "**/dll": true,
    "**/node_modules": true,
    "**/flow-typed": true,
    "*.log": true
  },
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": true,
    "**/flow-typed": true,
    "**/dist*": true,
    "**/.cache": true,
    "*.log": true
  },

  "bookmarks.navigateThroughAllFiles": true,
  "css.validate": false,
  "debug.allowBreakpointsEverywhere": false,
  "debug.inlineValues": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderIndicators": true,
  "diffEditor.renderSideBySide": true,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.autoClosingBrackets": true,
  "editor.codeLens": true,
  "editor.emptySelectionClipboard": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.folding": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.glyphMargin": true,
  "editor.hideCursorInOverviewRuler": false,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 80,
  "editor.overviewRulerLanes": 3,
  "editor.parameterHints": true,
  "editor.quickSuggestionsDelay": 300,
  "editor.renderControlCharacters": false,
  "editor.renderIndentGuides": false,
  "editor.renderLineHighlight": "line",
  "editor.renderWhitespace": "none",
  "editor.snippetSuggestions": "top",
  "editor.stablePeek": false,
  "editor.suggestOnTriggerCharacters": true,
  "editor.tabCompletion": false,
  "editor.trimAutoWhitespace": true,
  "editor.useTabStops": true,
  "editor.wordBasedSuggestions": false,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 120,
  // "explorer.openEditors.dynamicHeight": false,
  "explorer.openEditors.visible": 4,
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": true,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "html.format.enable": false,
  "html.suggest.html5": true,
  "html.validate.scripts": true,
  "html.validate.styles": true,
  "javascript.format.enable": false,
  "javascript.validate.enable": false,
  "json.format.enable": false,
  "problems.autoReveal": true,
  "python.venvPath": "~/.virtualenvs",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.formatting.provider": "yapf",
  "python.formatting.formatOnSave": true,
  "scss.validate": false,
  "search-node-modules.useLastFolder": true,

  "telemetry.enableCrashReporter": true,
  "telemetry.enableTelemetry": true,

  "terminal.integrated.shell.osx": "/bin/zsh",
  "tslint.autoFixOnSave": true,
  "tslint.enable": true,
  "typescript.format.enable": false,
  // "typescript.npm": "/Users/mac/.nvm/versions/node/v8.5.0/bin/npm",
  "typescript.referencesCodeLens.enabled": false,
  "window.newWindowDimensions": "inherit",
  "window.openFilesInNewWindow": "default",
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "all",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  // "workbench.colorCustomizations": {
  //   "tab.activeBackground": "#1245589f",
  //   "activityBar.background": "#1d82a79f",
  //   "editorGroup.background": "#1245589f",
  //   // "sideBar.background": "#c7e1eb9f",
  //   "sideBar.background": "#2669819f",
  //   "statusBar.background": "#1d82a79f"
  // },
  "workbench.quickOpen.closeOnFocusLost": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "newUntitledFile",
  "typescript.tsserver.log": "verbose",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "cSpell.language": "en",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": true,
  "workbench.editor.showTabs": true,
  "cSpell.userWords": [
    "bgcolor",
    "clsx",
    "comparator",
    "creatable",
    "datacenter",
    "datacenters",
    "dayjs",
    "dcim",
    "debounced",
    "denormalize",
    "denormalized",
    "describedby",
    "dropdown",
    "etixlabs",
    "firmwares",
    "formik",
    "geobox",
    "haspopup",
    "inputmode",
    "is",
    "lifecycle",
    "middlewares",
    "mixins",
    "nowrap",
    "organisation",
    "orgs",
    "oulet",
    "out",
    "protobuf",
    "qrcode",
    "react",
    "refetch",
    "refetching",
    "relogin",
    "repo",
    "rerender",
    "reselect",
    "selectable",
    "synapsia",
    "unmount",
    "workspaces",
    "xlocker"
  ],
  "versionlens.showVersionLensesAtStartup": false,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.rendererType": "dom",
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.iconTheme": "vscode-icons",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "One Monokai",
  "gitlens.codeLens.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "tabnine.experimentalAutoImports": true,
  "[shellscript]": {
    "files.eol": "\n"
  },
  "sqltools.highlightQuery": false,
  "gitlens.mode.active": "zen",
  "cSpell.enabled": true,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ]
}

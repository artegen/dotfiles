# Copied to ~/.ssh/config

Host github.com
	ControlMaster auto
	ControlPersist 120

Host git.example.com
  IdentityFile ~/.ssh/id_rsa-2

Host *
	# Use SSH2
	Protocol 2

	Compression yes
	TCPKeepAlive yes
	ServerAliveInterval 20
	ServerAliveCountMax 10
	AddKeysToAgent yes

	# Use a shared channel for all sessions to the same host
	ControlMaster auto
	ControlPath ~/.ssh/control/%r@%h:%p
	ControlPersist 1800

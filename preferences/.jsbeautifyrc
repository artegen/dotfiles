{
  "preserve_newlines": true,
  "max_preserve_newlines": 2,
  "wrap_line_length": 0,
  "end_with_newline": true,
  "indent_char": " ",
  "brace_style": "collapse,preserve-inline",

  # // "convert_quotes": "single",

  "html": {
    "ext": ["html", "xml", "svg"],
    "indent_handlebars": false,
    "indent_inner_html": false,
    # // "indent_scripts": "keep",
    "extra_liners": [
      "head",
      "body",
      "/html",
      "li",
      "ul",
      "ol",
      "input",
      "button",
      "form",
      "script",
      "iframe"
    ],
    "unformatted": [
      "a",
      "abbr",
      "area",
      "audio",
      "b",
      "bdi",
      "bdo",
      "cite",
      "code",
      "del",
      "dfn",
      "em",
      "embed",
      "i",
      "ins",
      "kbd",
      "keygen",
      "map",
      "mark",
      "math",
      "meter",
      "object",
      "output",
      "progress",
      "q",
      "ruby",
      "s",
      "samp",
      "time",
      "u",
      "var",
      "video",
      "wbr",
      "text",
      "acronym",
      "address",
      "big",
      "dt",
      "ins",
      "strike",
      "tt"
    ],
    "wrap_attributes": "forse",
    "indent_size": 4
  },

